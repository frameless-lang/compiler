state = \ -> {
	init = [Even(0), Odd(1), Even(2)]
	reducer = \ _, action -> action
}

main = \props ->
	let [values, dispatch] <- model(state());
	$header{
		onclick = \_ -> dispatch([Odd(0), Even(1), Even(2)])
	}
	each let Even(value) <- values
		$div
			"${value}"
	else
		$span
			"else"
	$footer
