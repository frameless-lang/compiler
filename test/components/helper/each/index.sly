#[webcomponent]
main : \{ foo: String[], baz: Boolean, bar: String }, {} -> Output
main = \props, _ ->
	$header
	if props.baz then
		if props.foo.length == 0 then
			$span
				"Empty list ${props.bar}"
		else
			let [value, index] <- props.foo.zip(0..);
			$div
				"${index}-${value}-${props.bar}"
	else
		$section
	$footer
