data State = First, Second(Integer), Third(Integer);

store = {
	init = First
	reducer : \States, Integer -> State
	reducer = \First, action -> Second[ action ]
	reducer = \Second[ value ], action -> Third[ value + action ]
}

main = \{}, {} -> View
main = \_, _ ->
	$div
		let [stateState, stateDispatch] <- model(store);
		match stateState
			case \First ->
				$button{
					onclick = \_ -> stateDispatch(1)
				}
					"first"
			case \Second[ value ] ->
				$button{
					onclick = \_ -> stateDispatch(2)
				}
					"second: ${value}"
			case \Third[ value ] ->
				"third: ${value}"
