data State = First, Second;

store = {
	init = First
	reducer : \State, State -> State 
	reducer = \_, action -> action
}

#[webcomponent]
main : \{ foo: Boolean, }, {} -> Output
main = \props, _ ->
	$header
	let [stateState, stateDispatch] <- model(store);
		if props.foo then
			match stateState
				case \First ->
					$div{
						onclick = \_ -> stateDispatch(Second)
					}
		else
			$section
	$footer
