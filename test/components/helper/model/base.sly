store = {
	init = ""
	reducer : \String, String -> String
	reducer = \_, action -> action
}

main : \ {}, {} -> Output
main = \_, _ ->
	let mainState = "";

	$input{
		type = "text"
		value = mainState
		oninput : \{ currentTarget : { value: String }} -> Void
		oninput = \evt -> mainState = evt.currentTarget.value;
	}
