store = {
	init = ""
	reducer : \String, String -> String
	reducer = \_, action -> action
}

#[webcomponent]
main : \ {}, {} -> Output
main = \_, _ ->
	let [mainState, mainDispatch] <- model(store);
	$input{
		type = "text"
		value = mainState
		oninput : \{ currentTarget : { value: String }} -> Void
		oninput = \value -> mainDispatch(value.currentTarget.value)
	}
