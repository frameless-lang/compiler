state = / -> {
	init = 0
	reducer = /state, Increment -> state + 1
	reducer = /state, Decrement -> state - 1
}

main = /props ->
	\[counterState, counterDispatch] <- model(state());
	$button{
		onclick = /_ -> counterDispatch(Decrement)
	}
		"Decrement"
	$button{
		onclick = /_ -> counterDispatch(Increment)
	}
		"Increment"
	"${counterState}"
