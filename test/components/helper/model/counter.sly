data Action = Increment, Decrement;

reducer : \Number, Action -> Number
reducer = \state, Increment -> state + 1
reducer = \state, Decrement -> state - 1

main : \ {}, {} -> Output
main = \_, _ ->
	let action: Action;
	let state = action:scan(reducer,  0);

	$button{
		onclick = \_ ->
			action = Decrement;
	}
		"Decrement"
	$button{
		onclick = \_ ->
			action = Increment;
	}
		"Increment"
	"${counterState}"
