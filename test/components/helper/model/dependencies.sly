data Action = RemoveSelection, AddSelection;

#[webcomponent]
main : \{ page: Number }, {} -> Output
main = \props, _ ->
	let entity <- [props.page * 10 .. (props.page * 10 + 10)];
	let selectionState = false

	$div{
		class?selected = selectionState
		onclick = \_ ->
			selectionState = if selectionState then False else True;
	}
		"${entity}"
