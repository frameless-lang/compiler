model:selection(
	init = _ -> false
	reducer = RemoveSelection _ = false
	reducer = AddSelection _ = false
)

view
	#each <entity, _> <- [page * 10 .. page * 10 + 10]
		#model selection <- selection(properties.page)
			div
			@class:selected=selection
			@onclick=(() -> selection(selection ? RemoveSelection : AddSelection)
				"${entity}"
	#else
		-
