model selection _
	false
	"REMOVE_SELECTION" = false
	"ADD_SELECTION" = false

view
	#each (entity, _) <- [page * 10 .. page * 10 + 10]
		#model selection properties.page
			div
			@class:selected=selection
			@onclick=(-> selection(selection ? "REMOVE_SELECTION" : "ADD_SELECTION", entity)
				"${entity}"
	#else
		-
