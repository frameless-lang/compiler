store = {
	init = [0, 1, 3]
	reducer  : \Integer[], Integer -> Integer[] 
	reducer = \state, multiply -> [ value, value * 2 | let value <- state;, value != 0]
}

main : \{}, {} -> View
main = \_, _ ->
	let [mainState, mainDispatch] <- model(store);
	$button{
		onclick = \_ -> mainDispatch(2)
	}
	let [value, index] <- zip(mainState, 0..);
	$div
		"${index}: ${value}"