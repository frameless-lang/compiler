import \vendor\strictly\prelude\src\index.js (zip)

state = \ -> {
	init = [0, 1, 3]
	reducer = \state, multiply -> [ value, value * 2 | let value <- state, value != 0]
}

main = \props ->
	let [mainState, mainDispatch] <- model(state());
	$button{
		onclick = \_ -> mainDispatch(2)
	}
	let [value, index] <- zip(mainState, 0..)
	$div
		"${index}: ${value}"