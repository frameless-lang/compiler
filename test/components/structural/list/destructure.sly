import \vendor\strictly\prelude\src\index.js (zipWithIndex)

main = \props ->
	match props.values
		case \[] ->
			$span
				"empty values"
		case \[value] ->
			$span
				"one value ${value}"
		case \[firstValue, secondValue | restValues] ->
			$span
				"first: ${firstValue}, second: ${secondValue}"
			each let [value, index] <- zipWithIndex(restValues)
				$div
					"${index}: ${value}"
			else
				$div
					"empty rest"
