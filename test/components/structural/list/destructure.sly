#[webcomponent]
main : \{ values: String[] }, {} -> Output
main = \props, _ ->
	match props.values
		case \[] ->
			$span
				"empty values"
		case \[value] ->
			$span
				"one value ${value}"
		case \[firstValue, secondValue | restValues] ->
			$span
				"first: ${firstValue}, second: ${secondValue}"
			let [value, index] <- restValues.zip(0..);
			$div
				"${index}: ${value}"
