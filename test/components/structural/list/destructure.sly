main = \props ->
	match props.values
		case \[] ->
			$span
				"empty values"
		case \[value] ->
			$span
				"one value ${value}"
		case \[firstValue, secondValue | restValues] ->
			$span
				"first: ${firstValue}, second: ${secondValue}"
			let [value, index] <- zip(restValues, [0..]);
			$div
				"${index}: ${value}"
