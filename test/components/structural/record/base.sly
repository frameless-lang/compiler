type State = { x: Ingeger, y: Integer };

store = {
	init = { x = 1, y = 0}
	reducer : \State, Integer -> State
	reducer = \state, x -> { x = state.x + x | state}
}

#[webcomponent]
main : \{}, {} -> Output
main = \_, _ ->
	let [mainState, mainDispatch] <- model(store);
	$button{
		onclick = \_ -> mainDispatch(2)
	}
	$div
		"${mainState.x} ${mainState.y}"
